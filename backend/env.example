CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=300

# Session Configuration
SESSION_SECRET=your_secure_session_secret_here_change_in_production 